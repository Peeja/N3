@prefix : <#> .
@prefix math: <http://www.w3.org/2000/10/swap/math#> .

# Integer divided by integer
{ ("2" "4") math:integerQuotient ?x } => {:test1a :is ?x}.
{ (2 4) math:integerQuotient ?x } => {:test1b :is ?x}.
{ (-2 4) math:integerQuotient ?x } => {:test1c :is ?x}.
{ (2 -4) math:integerQuotient ?x } => {:test1d :is ?x}.
{ (-2 -4) math:integerQuotient ?x } => {:test1e :is ?x}.
{ (0 2) math:integerQuotient ?x } => {:test1f :is ?x}.

# Combinations of types
{ (2.0 4) math:integerQuotient ?x } => {:test2a :is ?x}.
{ (16 0.5) math:integerQuotient ?x } => {:test2b :is ?x}.
{ (16 0.5e0) math:integerQuotient ?x } => {:test2c :is ?x}.
{ (3.1415926 1.0e7) math:integerQuotient ?x } => {:test2d :is ?x}.
{ (1.0e7 1729) math:integerQuotient ?x } => {:test2e :is ?x}.
{ (1.0e7 1.0e7) math:integerQuotient ?x } => {:test2f :is ?x}.
{ (1.0e7 10000000.0) math:integerQuotient ?x } => {:test2g :is ?x}.
{ (2.0 4) math:integerQuotient ?x } => {:test2h :is ?x}.

# Double
{ (0.0e0 3) math:integerQuotient ?x } => {:test3a :is ?x}.
{ (-0.0e0 3) math:integerQuotient ?x } => {:test3b :is ?x}.
{ (1.1e0 2.2e1) math:integerQuotient ?x } => {:test3c :is ?x}.
{ ("1.1e0" "2.2e1") math:integerQuotient ?x } => {:test3d :is ?x}.

# Argument errors
{ (1) math:integerQuotient ?x } => {:test4a a :FAILURE}.
{ (1 2 3) math:integerQuotient ?x } => {:test4b a :FAILURE}.
