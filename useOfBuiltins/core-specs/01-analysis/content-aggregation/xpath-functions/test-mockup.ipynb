    "from rdflib import Graph"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 42,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "@prefix : <urn:example:> .\n",
      "@prefix dc1: <http://purl.org/dc/terms/> .\n",
      "@prefix fno: <https://w3id.org/function/ontology#> .\n",
      "@prefix list: <https://w3id.org/function/ontology#List> .\n",
      "@prefix log: <http://www.w3.org/2000/10/swap/log#> .\n",
      "@prefix math: <http://www.w3.org/2000/10/swap/math#> .\n",
      "@prefix n3.list: <http://www.builtins/etc#> .\n",
      "@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .\n",
      "@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .\n",
      "\n",
      "n3.list:iterate a fno:Function ;\n",
      "    dc1:description \"\"\"Iterates over index/value pairs of the subject list. Gets pairs of index and element for every member of the subject.\n",
      "    \n",
      "    $s+ list:iterate $o[*]\n",
      "    For each element of $s, the builtin will unify $o with a list ( $e0 $e1 ) \n",
      "    where \n",
      "        $e0 [xsd:integer]: index in $s\n",
      "        $e1 [concrete]: element in $s\n",
      "    \n",
      "    \"\"\" ;\n",
      "    fno:constraints ( [ a fno:Constraint ;\n",
      "                dc1:description \"Domain can be a variable\" ;\n",
      "                fno:expression {\n",
      "                    {\n",
      "                        ?Subject log:bound false .\n",
      "\n",
      "                    } => {\n",
      "                            ?Subject ( :domain list:iterate ) true .\n",
      "\n",
      "                        } .\n",
      "\n",
      "                } ;\n",
      "                fno:prefixes ( [ a fno:Prefix ;\n",
      "                            fno:iri \"http://www.w3.org/2000/10/swap/log#\" ;\n",
      "                            fno:prefix \"log\" ] [ a fno:Prefix ;\n",
      "                            fno:iri \"http://www.w3.org/2000/10/swap/list#\" ;\n",
      "                            fno:prefix \"list\" ] [ a fno:Prefix ;\n",
      "                            fno:iri \"urn:\" ;\n",
      "                            fno:prefix \"\" ] ) ] [ a fno:Constraint ;\n",
      "                dc1:description \"Domain can be a list\" ;\n",
      "                fno:expression {\n",
      "                    {\n",
      "                        ?Subject log:rawType rdf:List .\n",
      "\n",
      "                    } => {\n",
      "                            ?Subject ( :domain list:iterate ) true .\n",
      "\n",
      "                        } .\n",
      "\n",
      "                } ;\n",
      "                fno:prefixes ( [ a fno:Prefix ;\n",
      "                            fno:iri \"http://www.w3.org/2000/10/swap/log#\" ;\n",
      "                            fno:prefix \"log\" ] [ a fno:Prefix ;\n",
      "                            fno:iri \"http://www.w3.org/2000/10/swap/list#\" ;\n",
      "                            fno:prefix \"list\" ] [ a fno:Prefix ;\n",
      "                            fno:iri \"urn:\" ;\n",
      "                            fno:prefix \"\" ] ) ] [ a fno:Constraint ;\n",
      "                dc1:description \"Range can be a variable.\" ;\n",
      "                fno:expression {\n",
      "                    {\n",
      "                        ?Object log:bound false .\n",
      "\n",
      "                    } => {\n",
      "                            ?Object ( :range list:iterate ) true .\n",
      "\n",
      "                        } .\n",
      "\n",
      "                } ;\n",
      "                fno:prefixes ( [ a fno:Prefix ;\n",
      "                            fno:iri \"http://www.w3.org/2000/10/swap/log#\" ;\n",
      "                            fno:prefix \"log\" ] [ a fno:Prefix ;\n",
      "                            fno:iri \"http://www.w3.org/2000/10/swap/list#\" ;\n",
      "                            fno:prefix \"list\" ] [ a fno:Prefix ;\n",
      "                            fno:iri \"urn:\" ;\n",
      "                            fno:prefix \"\" ] ) ] [ a fno:Constraint ;\n",
      "                dc1:description \"Range can be a list of length 2 with positive integer as first value, second value can be anything.\" ;\n",
      "                fno:expression {\n",
      "                    {\n",
      "                        ?Index log:rawType log:Literal ;\n",
      "                            math:greaterThan 0 .\n",
      "\n",
      "                        ?Object log:rawType rdf:List ;\n",
      "                            list:length 2 .\n",
      "\n",
      "                        [] rdf:first ?Index ;\n",
      "                            rdf:rest ( ?Value ) ;\n",
      "                            log:equalTo ?Object .\n",
      "\n",
      "                    } => {\n",
      "                            ?Object ( :range list:iterate ) true .\n",
      "\n",
      "                        } .\n",
      "\n",
      "                } ;\n",
      "                fno:prefixes ( [ a fno:Prefix ;\n",
      "                            fno:iri \"http://www.w3.org/2000/10/swap/log#\" ;\n",
      "                            fno:prefix \"log\" ] [ a fno:Prefix ;\n",
      "                            fno:iri \"http://www.w3.org/2000/10/swap/list#\" ;\n",
      "                            fno:prefix \"list\" ] [ a fno:Prefix ;\n",
      "                            fno:iri \"http://www.w3.org/2000/10/swap/math#\" ;\n",
      "                            fno:prefix \"math\" ] [ a fno:Prefix ;\n",
      "                            fno:iri \"urn:\" ;\n",
      "                            fno:prefix \"\" ] ) ] [ a fno:Constraint ;\n",
      "                dc1:description \"Range can be a list of length 2 with a variable as first value, second value can be anything.\" ;\n",
      "                fno:expression {\n",
      "                    {\n",
      "                        ?Index log:bound false .\n",
      "\n",
      "                        ?Object log:rawType rdf:List ;\n",
      "                            list:length 2 .\n",
      "\n",
      "                        [] rdf:first ?Index ;\n",
      "                            rdf:rest ( ?Value ) ;\n",
      "                            log:equalTo ?Object .\n",
      "\n",
      "                    } => {\n",
      "                            ?Object ( :range list:iterate ) true .\n",
      "\n",
      "                        } .\n",
      "\n",
      "                } ;\n",
      "                fno:prefixes ( [ a fno:Prefix ;\n",
      "                            fno:iri \"http://www.w3.org/2000/10/swap/log#\" ;\n",
      "                            fno:prefix \"log\" ] [ a fno:Prefix ;\n",
      "                            fno:iri \"http://www.w3.org/2000/10/swap/list#\" ;\n",
      "                            fno:prefix \"list\" ] [ a fno:Prefix ;\n",
      "                            fno:iri \"http://www.w3.org/2000/10/swap/math#\" ;\n",
      "                            fno:prefix \"math\" ] [ a fno:Prefix ;\n",
      "                            fno:iri \"urn:\" ;\n",
      "                            fno:prefix \"\" ] ) ] ) ;\n",
      "    fno:name \"concatentation\" ;\n",
      "    fno:object ( [ a rdf:Property ;\n",
      "                fno:parameter ( [ a fno:Output ;\n",
      "                            dc1:description \"index in $s\" ;\n",
      "                            fno:mode \"*\" ;\n",
      "                            fno:predicate \"$o.0\" ;\n",
      "                            fno:required true ;\n",
      "                            fno:type \"xs:integer\" ] [ a fno:Output ;\n",
      "                            dc1:description \"element in $s\" ;\n",
      "                            fno:mode \"*\" ;\n",
      "                            fno:predicate \"$o.1\" ;\n",
      "                            fno:required true ;\n",
      "                            fno:type \"concrete\" ] ) ] ) ;\n",
      "    fno:subject ( [ a rdf:Property ;\n",
      "                fno:parameter ( [ a fno:Parameter ;\n",
      "                            dc1:description \"\" ;\n",
      "                            fno:mode \"+\" ;\n",
      "                            fno:predicate \"$s\" ;\n",
      "                            fno:required true ;\n",
      "                            fno:type log:list ] ) ] ) ;\n",
      "    fno:validation [ a fno:Test ;\n",
      "            fno:expression {\n",
      "                {\n",
      "                    ?Test a :TestCase ;\n",
      "                        rdf:object ?Object ;\n",
      "                        rdf:subject ?Subject ;\n",
      "                        :forBuiltIn list:iterate .\n",
      "\n",
      "                    ?Object ( :range list:iterate ) true .\n",
      "\n",
      "                    ?Subject ( :domain list:iterate ) true .\n",
      "\n",
      "                } => {\n",
      "                        ?Test a log:Valid .\n",
      "\n",
      "                    } .\n",
      "\n",
      "            } ;\n",
      "            fno:testCases ( [ a fno:TestCase ;\n",
      "                        dc1:description \"\" ;\n",
      "                        fno:data {\n",
      "                            :test1 rdf:object ( 1 \"Tim\" ) ;\n",
      "                                rdf:subject ?List .\n",
      "\n",
      "                        } ;\n",
      "                        fno:expectedResult true ] [ a fno:TestCase ;\n",
      "                        dc1:description \"\" ;\n",
      "                        fno:data {\n",
      "                            :test2 rdf:object ( 1 \"William\" ) ;\n",
      "                                rdf:subject ( \"William\" \"Tim\" \"Patrick\" ) .\n",
      "\n",
      "                        } ;\n",
      "                        fno:expectedResult true ] [ a fno:TestCase ;\n",
      "                        dc1:description \"\" ;\n",
      "                        fno:data {\n",
      "                            :test3 rdf:object ( ?V \"Tim\" ) ;\n",
      "                                rdf:subject ( \"William\" \"Tim\" \"Patrick\" ) .\n",
      "\n",
      "                        } ;\n",
      "                        fno:expectedResult true ] [ a fno:TestCase ;\n",
      "                        dc1:description \"\" ;\n",
      "                        fno:data {\n",
      "                            :test4 rdf:object ( 3 \"Patrick\" ) ;\n",
      "                                rdf:subject ( \"William\" \"Tim\" \"Patrick\" ) .\n",
      "\n",
      "                        } ;\n",
      "                        fno:expectedResult true ] [ a fno:TestCase ;\n",
      "                        dc1:description \"\" ;\n",
      "                        fno:data {\n",
      "                            :test5 rdf:object ?V ;\n",
      "                                rdf:subject ( \"William\" \"Tim\" \"Patrick\" ) .\n",
      "\n",
      "                        } ;\n",
      "                        fno:expectedResult true ] [ a fno:TestCase ;\n",
      "                        dc1:description \"\" ;\n",
      "                        fno:data {\n",
      "                            :test5 rdf:object ( \"a3\" \"Patrick\" ) ;\n",
      "                                rdf:subject ( \"William\" \"Tim\" \"Patrick\" ) .\n",
      "\n",
      "                        } ;\n",
      "                        fno:expectedResult false ] [ a fno:TestCase ;\n",
      "                        dc1:description \"\" ;\n",
      "                        fno:data {\n",
      "                            :test5 rdf:object ( \"a3\" \"Patrick\" ) ;\n",
      "                                rdf:subject () .\n",
      "\n",
      "                        } ;\n",
      "                        fno:expectedResult false ] ) ] .\n",
      "\n",
      "\n"
     ]
    }
   ],
   "source": [
    "g = Graph().parse('list.iterate.fno.n3', format='n3')\n",
    "g.bind('dc', 'http://purl.org/dc/terms/')\n",
    "g.print(format='n3')"
   ]
