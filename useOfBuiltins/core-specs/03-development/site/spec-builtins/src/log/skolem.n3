@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .

log:skolem a fno:Function ;
    vs:term_status  "testing" ;
    dcterms:comment "Built-in to generate a Skolem IRI object which is a function of the arguments in the subject list" ;
    fno:example ( [ a fno:Test ;
                dcterms:description """
Generate a unique Skolem IRI from the list (:abc 77 "xyz") .
            """ ;
                rdfs:seeAlso <> ;
                fno:expression """
@prefix : <http://example.org/#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

{ (:abc 77 "xyz") log:skolem ?skolem . } => { :result :is ?skolem } .
            """ ;
                fno:result """
:result :is <http://www.w3.org/2000/10/swap/genid#zmgk3Vt_z_u7FQlk1NmqIw> . 
            """ ] ) ;
    fno:name "skolem" ;
    fno:object ( [ a rdf:Property ;
                fno:parameter ( [ a fno:Output ;
                            dcterms:description "" ;
                            fno:mode "*" ;
                            fno:predicate "$o.0" ;
                            fno:required true ;
                            fno:type log:String ] ) ] ) ;
    fno:subject ( [ a rdf:Property ;
                fno:parameter ( [ a fno:Parameter ;
                            dcterms:description "" ;
                            fno:mode "+" ;
                            fno:predicate "$s" ;
                            fno:required true ;
                            fno:type log:List ] ) ] ) .
