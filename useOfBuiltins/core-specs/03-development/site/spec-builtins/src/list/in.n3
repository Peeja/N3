@prefix list: <http://www.w3.org/2000/10/swap/list#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .

list:in a fno:Function ;
    vs:term_status  "testing" ;
    dcterms:comment """Determines if an subject is a member of the object list or binds every member of the object list to the subject.
`true` if and only `$o` is a list and `$s` is in that list.
`$s` can be calculated from `$o`.""" ;
    fno:example ( 
            [ a fno:Test ;
                dcterms:description """
Does ( "dog" "penguin" "cat" ) contain a "cat"?
            """ ;
                rdfs:seeAlso <> ;
                fno:expression """
@prefix : <http://example.org/#>.
@prefix list: <http://www.w3.org/2000/10/swap/list#> .

{ "cat" list:in ( "dog" "penguin" "cat" ) . } => { :result :is true . } .
            """ ;
                fno:result """
:result :is true. 
            """ ] 

            [ a fno:Test ;
                dcterms:description """
What are the members of ( "dog" "penguin" "cat" )?
            """ ;
                rdfs:seeAlso <> ;
                fno:expression """
@prefix : <http://example.org/#>.
@prefix list: <http://www.w3.org/2000/10/swap/list#> .

{ ?what list:in ( "dog" "penguin" "cat" ) . } => { :result :is ?what . } .
            """ ;
                fno:result """
:result :is "dog" .
:result :is "pengiun" . 
:result :is "cat" .
            """ ] 

    ) ;
    fno:name "in" ;
    fno:object ( [ a rdf:Property ;
                fno:parameter ( [ a fno:Output ;
                            dcterms:description "" ;
                            fno:mode "*" ;
                            fno:predicate "$o.0" ;
                            fno:required true ;
                            fno:type rdf:List ] ) ] ) .
